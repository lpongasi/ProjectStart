{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "typeRoots": ["types"],
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "noFallthroughCasesInSwitch": true,
    "noEmit": true,
    "allowJs": true,
    "experimentalDecorators": true,
    "sourceMap": false,
    "strict": false,
    "noEmitOnError": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "lib": [ "es5", "es6", "es2017.object", "dom" ],
    "paths": {
      "shared/*": [ "./ClientApp/Shared/*" ],
      "component/*": [ "./ClientApp/Component/*" ],
      "container/*": [ "./ClientApp/Container/*" ]
    },
    "outDir": "./wwwroot/clientapp"
  },
  "exclude": [
    "bin",
    "node_modules"
  ],
  "include": ["./ClientApp/**/*"]
}